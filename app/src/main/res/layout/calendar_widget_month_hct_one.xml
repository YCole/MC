                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:includeFontPadding="false"
                android:textSize="@dimen/month_week_day_bottom_text_size"
                android:visibility="gone" />
        </LinearLayout>

        <TextView
            android:id="@+id/agenda_event_color_11"
            style="@style/WidgetMonthEvent"
            android:layout_width="10dp"
            android:layout_height="10dp"
            android:layout_above="@id/typeb_calendar_num"
            android:layout_marginBottom="-1dp"
            android:layout_marginStart="19dp"
            android:background="@drawable/widget_week_background"
            android:gravity="center"
            android:textSize="8dp" />

        <ImageView
            android:id="@+id/weather_icon_11"
            android:layout_width="12dp"
            android:layout_height="12dp"
            android:layout_above="@id/typeb_calendar_num"
            android:layout_toRightOf="@id/agenda_event_color_11" />

        <ImageView
            android:id="@+id/agenda_todo_color_11"
            style="@style/WidgetMonthTodo"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/typeb_calendar_num"
            android:layout_marginTop="2dp"
            android:gravity="center"
            android:src="@drawable/widget_event_circle_draw" />
    </RelativeLayout>

