                android:drawable="@drawable/checkbox_00005"
                android:duration="30" />
            <item
                android:drawable="@drawable/checkbox_00006"
                android:duration="30" />
            <item
                android:drawable="@drawable/checkbox_00007"
                android:duration="30" />
            <item
                android:drawable="@drawable/checkbox_00008"
                android:duration="30" />
            <item
                android:drawable="@drawable/checkbox_00009"
                android:duration="30" />
            <item
                android:drawable="@drawable/checkbox_00010"
                android:duration="30" />
            <item
                android:drawable="@drawable/checkbox_00011"
                android:duration="30" />
            <item
                android:drawable="@drawable/checkbox_00012"
                android:duration="30" />
        </animation-list>
    </transition>
</animated-selector>