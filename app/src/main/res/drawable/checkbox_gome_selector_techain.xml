<?xml version="1.0" encoding="utf-8"?>
<animated-selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:id="@+id/state_off"
        android:state_checked="false"
        android:state_enabled="true"
        android:drawable="@drawable/checkbox_00001" />

    <item
        android:id="@+id/state_on"
        android:state_enabled="true"
        android:state_checked="true"
        android:drawable="@drawable/checkbox_00012" />

    <item android:drawable="@drawable/checkbox_on_disable" android:state_enabled="false" android:state_checked="true"></item>
    <item android:drawable="@drawable/checkbox_off_disable" android:state_enabled="false" android:state_checked="false"></item>
    <transition
        android:fromId="@id/state_on"
        android:toId="@id/state_off">
        <animation-list>
            <item
                android:drawable="@drawable/checkbox_00030"
                android:duration="30" />
            <item
                android:drawable="@drawable/checkbox_00031"
                android:duration="30" />
            <item
                android:drawable="@drawable/checkbox_00032"
                android:duration="30" />
            <item
                android:drawable="@drawable/checkbox_00033"
                android:duration="30" />
            <item
                android:drawable="@drawable/checkbox_00034"
                android:duration="30" />
            <item
                android:drawable="@drawable/checkbox_00035"
                android:duration="30" />
            <item
                android:drawable="@drawable/checkbox_00036"
                android:duration="30" />
            <item
                android:drawable="@drawable/checkbox_00037"
                android:duration="30" />
            <item
                android:drawable="@drawable/checkbox_00038"
                android:duration="30" />
            <item
                android:drawable="@drawable/checkbox_00039"
                android:duration="30" />
            <item
                android:drawable="@drawable/checkbox_00040"
                android:duration="30" />
            <item
                android:drawable="@drawable/checkbox_00041"
                android:duration="30" />
            <item
                android:drawable="@drawable/checkbox_00042"
                android:duration="30" />
        </animation-list>
    </transition>

    <transition
        android:fromId="@id/state_off"
        android:toId="@id/state_on">
        <animation-list>
            <item
                android:drawable="@drawable/checkbox_00000"
                android:duration="30" />
            <item
                android:drawable="@drawable/checkbox_00001"
                android:duration="30" />
            <item
                android:drawable="@drawable/checkbox_00002"
                android:duration="30" />
            <item
                android:drawable="@drawable/checkbox_00003"
                android:duration="30" />
            <item
                android:drawable="@drawable/checkbox_00004"
                android:duration="30" />
